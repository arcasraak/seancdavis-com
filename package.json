{
  "name": "seancdavis-me",
  "version": "1.0.0",
  "author": "Sean C Davis <scdavis41@gmail.com>",
  "dependencies": {
    "@11ty/eleventy": "^0.10.0",
    "@fullhuman/postcss-purgecss": "^2.1.2",
    "autoprefixer": "^9.7.6",
    "cssnano": "^4.1.10",
    "glob": "^7.1.6",
    "html-minifier": "^4.0.0",
    "imgix-core-js": "^2.3.1",
    "jest": "^26.2.2",
    "lodash": "^4.17.19",
    "netlify-plugin-subfont": "^5.0.6",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.1",
    "postcss-custom-properties": "^9.1.1",
    "postcss-import": "^12.0.1",
    "postcss-nested": "^4.2.3",
    "tailwindcss": "^1.4.4",
    "webpack": "^4.44.0",
    "webpack-cli": "^3.3.12"
  },
  "scripts": {
    "build": "npm-run-all clean build:css build:html build:js",
    "build:css": "postcss src/_includes/css/index.css -o src/css/styles.css",
    "build:html": "eleventy",
    "build:js": "webpack",
    "clean": "rm -rf dist",
    "develop": "npm-run-all --parallel develop:css develop:html develop:js",
    "develop:css": "postcss src/_includes/css/index.css -o src/css/styles.css --watch",
    "develop:html": "eleventy --serve --port 8000",
    "develop:js": "webpack",
    "start": "develop",
    "test": "jest"
  }
}
